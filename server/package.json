      "@types/node": "^8.0.29",
      "ts-node": "3.3.0",
      "ts-node-dev": "^1.0.0-pre.56",
      "tsconfig-paths": "^3.9.0",
      "typescript": "^3.9.7"
   },
   "dependencies": {
      "bcrypt": "^5.0.0",
      "cors": "^2.8.5",
      "express": "^4.17.1",
      "jsonwebtoken": "^8.5.1",
      "knex": "^0.21.2",
      "mysql": "^2.14.1",
      "reflect-metadata": "^0.1.13",
      "sqlite3": "^5.0.0",
      "typeorm": "0.2.25"
   }
